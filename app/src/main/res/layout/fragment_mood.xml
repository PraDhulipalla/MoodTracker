<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="beta.drab.moodtracker.Fragments.MoodFragment">

    <com.melnykov.fab.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|center_vertical"
        android:layout_marginTop="43dp"
        android:elevation="10dp"
        android:layout_below="@+id/fab2"
        android:layout_alignLeft="@+id/fab2"
        android:layout_alignStart="@+id/fab2" />

    <com.melnykov.fab.FloatingActionButton
        android:id="@+id/fab2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left|center_vertical"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="99dp"
        android:elevation="10dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="66dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD MOOD"
        android:id="@+id/textView"
        android:textSize="25dp"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:textColor="#fff9fff4"
        android:singleLine="false"
        android:layout_alignTop="@+id/fab2"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:shadowColor="#ff000000"
        android:shadowDx="-2"
        android:shadowDy="2"
        android:shadowRadius="0.01"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MODIFY MOOD"
        android:id="@+id/textView2"
        android:textSize="25dp"
        android:shadowColor="#ff000000"
        android:shadowDx="-2"
        android:shadowDy="2"
        android:shadowRadius="0.01"
        android:layout_marginBottom="11dp"
        android:textColor="#fff9fff4"
        android:textStyle="bold"
        android:phoneNumber="false"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_alignBottom="@+id/fab"
        android:layout_toRightOf="@+id/fab"
        android:layout_toEndOf="@+id/fab" />


</RelativeLayout>
